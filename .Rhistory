geom_area(data = dat |> filter(x > qnorm(.95)), aes(x = x, y = y), fill = 'skyblue3', alpha = .6) +
geom_line() +
geomtextpath::geom_textvline(aes(xintercept = qnorm(.95), label = "2.5% cutoff"),
hjust = .5, size = 6, color = "skyblue3", linewidth = 1.5) +
geomtextpath::geom_textvline(aes(xintercept = qnorm(.97), label = "observed t-stat"),
hjust = .5, size = 6, color = "gray", linewidth = 1.5) +
scale_x_continuous(breaks = round(c(qnorm(.97), qnorm(.025), 0, qnorm(.975)), 2)) +
coord_cartesian(expand = F, clip = "off") +
guides(y = "none", x = NULL) +
labs(y = NULL) +
theme_classic(base_size = 12)
